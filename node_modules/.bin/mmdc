#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules/@mermaid-js/mermaid-cli/src/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules/@mermaid-js/mermaid-cli/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules/@mermaid-js/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules/@mermaid-js/mermaid-cli/src/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules/@mermaid-js/mermaid-cli/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules/@mermaid-js/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/@mermaid-js+mermaid-cli@11.12.0_puppeteer@23.11.1_tsx@4.21.0/node_modules:/Users/bradygeorgen/Documents/workspace/stldnb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mermaid-js/mermaid-cli/src/cli.js" "$@"
else
  exec node  "$basedir/../@mermaid-js/mermaid-cli/src/cli.js" "$@"
fi
